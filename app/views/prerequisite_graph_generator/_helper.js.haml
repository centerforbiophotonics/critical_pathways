  var Helper = function(){}

  Helper.prototype.isArray = function(obj) { return (Object.prototype.toString.call(obj) === '[object Array]') }

  Helper.prototype.spliceMultInds = function(arr, indsToRemove){ 
    return $.grep(arr, function(n, i) {
      return $.inArray(i, indsToRemove) === -1;
    });
  }

  Helper.prototype.alphaNumericCmp = function(a, b){
    return a.split(' ').join('').toLowerCase() === b.split(' ').join('').toLowerCase();
  }

  Helper.prototype.alphabeticalCmp =  function(a, b) {
    var A = a.text.toLowerCase().split('');
    var B = b.text.toLowerCase().split('');
    var len = Math.min(A.length, B.length);

    for(var i = 0; i < len; i++){
      if(A[i] < B[i])
        return -1;
      else if (A[i] > B[i])
        return 1;
    }
    if(A.length < B.length)
      return -1;
    else if(A.length > B.length)
      return 1;

    return 0;
  }

  Helper.prototype.numDigits = function(str){
    var strArray = str.split(''), numDigits = 0;

    for(var i = 0; i < strArray.length; i++)
      if(!(strArray[i] >= "0" && strArray[i] <= "9"))
        break;

    return i;
  }